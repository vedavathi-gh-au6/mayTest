{{> header}}
<div class="container">
    <div class="row mt-5">
        {{#each blogs}}
        <div class="col-8 mx-auto">
            <div class="form-group">
                <h5>{{this.title}}</h5>
            </div>
            <div class="form-group">
                <textarea class="form-control" id="exampleFormControlTextarea1" rows="3"
                    disabled>{{this.content}}</textarea>
            </div>
        </div>
        {{/each}}
    </div>
</div>